    "def greeting(func):\n",
    "    def wrapper():\n",
    "        print(\"Hello, Nice to meet you.\")\n",
    "        func()\n",
    "        print(\"It feels so good to meet you, Goodbye.\")\n",
    "    return wrapper"
   ]
  },
